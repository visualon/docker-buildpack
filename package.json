    "npm-run-all2": "7.0.2",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.10",
    "semantic-release": "24.2.3",
    "shelljs": "0.9.1",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.6.2",
  "engines": {
    "node": "^22.11.0",
    "pnpm": "^10.0.0"
